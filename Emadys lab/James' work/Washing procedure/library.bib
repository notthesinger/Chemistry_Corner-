journal = {Enzyme and Microbial Technology},
keywords = {Candida bombicola growth,Sophorolipids,factorial design of experiments},
number = {3},
pages = {221--229},
title = {{Optimization of a synthetic medium for Candida bombicola growth using factorial design of experiments}},

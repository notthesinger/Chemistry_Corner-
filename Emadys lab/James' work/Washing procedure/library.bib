keywords = {Alkyl esters,Crystallization,Jojoba oil,Jojobyl alcohols,Transesterification},
pages = {244--250},
publisher = {Elsevier B.V.},
title = {{Fatty acid alkyl esters and monounsaturated alcohols production from jojoba oil using short-chain alcohols for biorefinery concepts}},
url = {http://dx.doi.org/10.1016/j.indcrop.2015.02.031},

issn = {09603085},
journal = {Food and Bioproducts Processing},
keywords = {Antioxidant,Bioactive,Esterification,Lipase,Lipoic acid,Trehalose},
pages = {163--172},
publisher = {Institution of Chemical Engineers},
title = {{Enzymatic synthesis, purification and identification of bioactive trehalose ester derivatives for health applications}},

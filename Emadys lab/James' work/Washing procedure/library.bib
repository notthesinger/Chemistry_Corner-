number = {1},
pages = {239--247},
pmid = {21225383},
title = {{Novel Jojoba Oil-Based Emulsion Gel Formulations for Clotrimazole Delivery}},
url = {http://www.springerlink.com/index/10.1208/s12249-011-9583-4},

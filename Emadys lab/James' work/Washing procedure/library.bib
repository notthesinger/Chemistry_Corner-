file = {:C$\backslash$:/Users/james/Google Drive/Sonoran Desert Research/Transesterification/Lipase catalyzed Transesterification/Effective immobilisation of lipase to enhance esterification potential and reusability.pdf:pdf},
issn = {03666352},

issn = {02632241},
journal = {Measurement: Journal of the International Measurement Confederation},
keywords = {Angle of repose,Granular material,Measurement uncertainty,Photogrammetry},
number = {July 2017},
pages = {19--26},
title = {{The use of the photogrammetric method for measurement of the repose angle of granular materials}},
volume = {115},
year = {2018}
}

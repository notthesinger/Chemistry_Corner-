issn = {09396411},
journal = {European Journal of Pharmaceutics and Biopharmaceutics},
keywords = {EPR,Electron paramagnetic resonance,Lipid drug delivery,Microviscosity,Nanodispersion,Sucrose ester,Sugar ester,Viscoelastic behaviour},
number = {2},
pages = {550--555},
pmid = {18588974},
title = {{Sucrose ester nanodispersions: Microviscosity and viscoelastic properties}},

@article{Wang2012,
abstract = {An array of ten $\beta$-D-glucopyranosides with varied alkyl chain lengths were enzymatically synthesized. It was found that for longer alkyl chains a lower initial rate and final yield of glucoside was obtained except for methyl glucoside because of the severe toxicity of methanol to the enzyme. From a thermodynamics point of view, the equilibrium constant and Gibbs free energy variation of the glucoside syntheses were systematically investigated. To improve the final yields of the glucosides containing long alkyl chains the equilibrium of the enzymatic glucoside synthesis was altered. The equilibrium yield of decyl $\beta$-D-glucoside increased from 1.9{\%} to 6.1{\%} when the water content was reduced from 10{\%} to 5{\%} (v/v) using tert-butanol as a cosolvent and 0.10 mol/L of glucose as a substrate. As for the other longer alkyl chain glucosides, heptyl $\beta$-D-glucoside was found to have significant surface activity as well.},
author = {Wang, Qinqqin and Yu, Huilei and Zhao, Na and Li, Chunxiu and Shang, Yazhuo and Liu, Honglai and Xu, Jianhe},
doi = {10.1016/S1872-2067(11)60333-1},
file = {:C$\backslash$:/Users/james/Google Drive/Sonoran Desert Research/Glycosylation Methods/Enzymatic Glycosylation/Long-chain glycoside formation with almond meal.pdf:pdf},
isbn = {0253-9837},
issn = {02539837},
journal = {Cuihua Xuebao/Chinese Journal of Catalysis},
keywords = {Alkyl $\beta$-D-glucosides,Decyl $\beta$-D-glucoside,Enzymatic synthesis,Equilibrium yield,Reverse hydrolysis,Water-poor system},
number = {2},
pages = {275--280},
publisher = {Dalian Institute of Chemical Physics, the Chinese Academy of Sciences},
title = {{Significantly improved equilibrium yield of long-chain alkyl glucosides via reverse hydrolysis in a water-poor system using cross-linked almond meal as a cheap and robust biocatalyst}},
url = {http://dx.doi.org/10.1016/S1872-2067(11)60333-1},
volume = {33},
year = {2012}
}
@article{Ferrer2005a,
abstract = {The lipases from Thermomyces lanuginosus (immobilized by granulation with silica) and Candida antarctica B (adsorbed on Lewatit, "Novozym 435") were comparatively assayed for the synthesis of sugar esters by transesterification of sugars with fatty acid vinyl esters in 2-methyl-2-butanol:dimethylsulfoxide mixtures. We found that lipase from C. antarctica B is particularly useful for the preparation of 6,6′-di- acylsucrose, whereas T. lanuginosus lipase catalyzes selectively the synthesis of 6-O-acylsucrose. The granulated T. lanuginosus lipase retained more than 80{\%} of its initial activity after 20 cycles of 6 h. Both lipases were similarly effective for the regioselective synthesis of 6′-O-palmitoylmaltose and 6-O-lauroylglucose. The effect of the synthesized sugar esters on the growth in liquid medium of various microorganisms (Gram-positive, Gram-negative and yeasts) was evaluated. 6-O-lauroylsucrose and 6′-O-lauroylmaltose inhibited the growth of Bacillus sp. at a concentration of 0.8 mg/ml, and of Lactobacillus plantarum at 4 mg/ml. Sucrose dilaurates and 6-O-lauroylglucose did not show antimicrobial activity, probably due to their low aqueous solubility. As regards the inhibition of yeasts, none of the tested carbohydrate esters inhibited significantly the growth of Zygosaccharomyces rouxii and Pichia jadinii. {\textcopyright} 2004 Elsevier Inc. All rights reserved.},
author = {Ferrer, Manuel and Soliveri, Juan and Plou, Francisco J. and L{\'{o}}pez-Cort{\'{e}}s, Nieves and Reyes-Duarte, Dolores and Christensen, Morten and Copa-Pati{\~{n}}o, Jos{\'{e}} L. and Ballesteros, Antonio},
doi = {10.1016/j.enzmictec.2004.02.009},
file = {:C$\backslash$:/Users/james/Google Drive/Sonoran Desert Research/Sucrose Esters/Enzyme Based Synthesis of Sucrose Esters/Synthesis of sugar esters in solvent mixtures by lipases from Thermomyces lanuginosus and Candida antarctica B, and their antimicrobial properties.pdf:pdf},
isbn = {0141-0229},
issn = {01410229},
journal = {Enzyme and Microbial Technology},
keywords = {Antimicrobial activity,Carbohydrate esters,Enzymatic transesterification,Enzyme granulation,Glucose esters,Lipases,Maltose esters,Sucrose esters,Vinyl fatty acid esters},
number = {4},
pages = {391--398},
title = {{Synthesis of sugar esters in solvent mixtures by lipases from Thermomyces lanuginosus and Candida antarctica B, and their antimicrobial properties}},
volume = {36},
year = {2005}
}

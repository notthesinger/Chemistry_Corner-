isbn = {0168-1656 (Print)$\backslash$r0168-1656 (Linking)},
issn = {01681656},
journal = {Journal of Biotechnology},
keywords = {Apple seed meal,Biocatalysis,Glycoside synthesis,Green chemistry,Salidroside},
number = {4},
pages = {469--477},
pmid = {18207272},
title = {{Environmentally benign synthesis of natural glycosides using apple seed meal as green and robust biocatalyst}},
volume = {133},
year = {2008}

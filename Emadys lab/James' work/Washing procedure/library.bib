@article{Guo2015,
abstract = {The novel whole-cell biocatalyst Candida antarctica lipase B displaying- Pichia pastoris (Pp-CALB) is characterized by its low preparation cost and could be an alternative to the commercial immobilized Candida antarctica lipase B (CALB). This study addresses the feasibility of using Pp-CALB in large scale glucose fatty acid esters production. 1,2-O-Isopropylidene-$\alpha$- d-glucofuranose (IpGlc) was used as the acyl acceptor to overcome the low solubility of glucose in an organic solvent and to avoid the addition of toxic co-solvents. IpGlc significantly improved the Pp-CALB catalyzing esterification efficiency when using long chain fatty acids as the acyl donor. Under the preferred operating conditions (50. °C, 40. g/L molecular sieve dosage and 200. rpm mixing intensity), 60.5{\%} of IpGlc converted to 6-O-myristate-1, 2-O-isopropylidene-$\alpha$- d-glucofuranose (C14-IpGlc) after a 96-h reaction in a 2-L stirred reactor. In a 5-L pilot scale test, Pp-CALB also showed a similar substrate conversion rate of 55.4{\%} and excellent operational stability. After C14-IpGlc was collected, 70{\%} trifluoroacetic acid was adopted to hydrolyze C14-IpGlc to myristate glucose ester (C14-Glc) with a high yield of 95.3{\%}. In conclusion, Pp-CALB is a powerful biocatalyst available for industrial synthesis, and this study describes an applicable and economical process for the large scale production of myristate glucose ester.},
author = {Guo, Dong Heng and Jin, Zi and Xu, Yan Shan and Wang, Ping and Lin, Ying and Han, Shuang Yan and Zheng, Sui Ping},
doi = {10.1016/j.enzmictec.2015.04.002},
file = {:C$\backslash$:/Users/james/Google Drive/Sonoran Desert Research/Sucrose Esters/Sucrose Ester Synthesis/Scaling-up the synthesis of myristate glucose ester catalyzed by a.pdf:pdf},
issn = {18790909},
journal = {Enzyme and Microbial Technology},
keywords = {1,2-O-Isopropylidene-$\alpha$-d-glucofuranose,Candida antarctica lipase B,Myristate glucose ester,Scale synthesis,Yeast whole cell biocatalyst},
pages = {30--36},
publisher = {Elsevier Inc.},
title = {{Scaling-up the synthesis of myristate glucose ester catalyzed by a CALB-displaying Pichia pastoris whole-cell biocatalyst}},
url = {http://dx.doi.org/10.1016/j.enzmictec.2015.04.002},
volume = {75-76},
year = {2015}
}
@book{Situation2012a,
abstract = {Publisher Summary This chapter summarizes crystal growth, final finish machining, precision cleaning, and defect inspection of a substrate by using a semiconductor silicon single crystal destined for an LSI as an example. The engineering methods for producing a single crystal are broadly classified into two types: the Czochralski (CZ) method and the Floating Zone (FZ) method. Both involve careful solidification of molten silicon, which has been previously made from heated polycrystalline silicon. A round rod or square pillar of single crystal silicon, called a seed crystal, is prepared. This is brought down into the molten silicon and, soon after it touches the surface, the interface instantly solidifies. The molten silicon that is contacted solidifies as a single crystal. Current wire saws can cut up to four ingots simultaneously from four sides of the web; a vast improvement in productivity over the inner diameter (ID) saw. Since the wire is so thin, the kerf loss in the wire saw cutting process is minimal.},
annote = {Base Etching - p.88-90},
author = {Situation, The Current and Technology, Ultra-precision and Crystals, Silicon Single},
booktitle = {Advances},
doi = {10.1016/B978-1-4377-7859-5.00003-X},

journal = {Journal of Chemical Technology {\&} Biotechnology},
keywords = {Peracetico. Ca{\~{n}}a de azucar},
number = {May},
pages = {1115--1121},
pmid = {17034937},
title = {{Effect of several factors on peracetic acid pretreatment of sugarcane bagasse for enzymatic hydrolysis}},
volume = {82},
year = {2007}
}
@article{Crucho2015,
